{
	"name": "audiobus",
	"version": "0.15.1",
	"description": "Core audio engine with MIDI processing, synthesis, and audio routing",
	"type": "module",
	"main": "index.ts",
	"exports": {
		".": "./index.ts",
		"./commands": "./commands.ts",
		"./instruments": "./instruments/index.ts",
		"./instruments/*": "./instruments/*",
		"./io": "./io/index.ts",
		"./io/*": "./io/*",
		"./midi": "./midi/index.ts",
		"./midi/*": "./midi/*",
		"./timing": "./timing/index.ts",
		"./timing/*": "./timing/*",
		"./tuning": "./tuning/index.ts",
		"./tuning/*": "./tuning/*",
		"./conversion": "./conversion/index.ts",
		"./conversion/*": "./conversion/*",
		"./storage": "./storage/index.ts",
		"./storage/*": "./storage/*",
		"./exporters/*": "./exporters/*",
		"./importers/*": "./importers/*",
		"./ui/*": "./ui/*",
		"./hardware/*": "./hardware/*",
		"./visualisation/*": "./visualisation/*",
		"./*": "./*"
	},
	"scripts": {
		"dev": "echo 'No dev server for this package'"
	},
	"files": [
		"*.ts",
		"*.js",
		"instruments",
		"io",
		"midi",
		"timing",
		"tuning",
		"conversion",
		"storage",
		"exporters",
		"importers",
		"ui",
		"hardware",
		"visualisation"
	],
	"dependencies": {
		"pitfalls": "workspace:^",
		"@msgpack/msgpack": "^3.1.3",
		"@tonejs/midi": "^2.0.27",
		"fflate": "^0.8.2",
		"jzz": "^1.9.6",
		 "lz-string": "^1.5.0",
		"standardized-audio-context": "^25.3.77",
		"supersonic-scsynth-bundle": "^0.42.0",
		"vexflow": "^4.2.2",
		"wam-community": "^0.4.9",
		"webmidi": "^3.1.14"
	},
	"devDependencies": {
		"typescript": "~5.9.3"
	}
}